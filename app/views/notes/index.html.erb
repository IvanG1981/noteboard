


    <%= link_to new_note_path do %>
      <span >
          Write Note
      </span>
    <% end %>
    <div >
        <% @notes.each do |note| %>
        <div >
          <span>
            <i><%= time_ago_in_words(note.created_at) %></i>
          </span>
          <p>
            <%= note.content %>
          </p>
          <span>
            <%= note.created_at %>
          </span>
        </div>
      <% end %>
    </div>
